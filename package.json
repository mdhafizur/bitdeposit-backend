    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test,prisma}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:debug:prisma": "concurrently --kill-others \"nest start --watch\" \"npx prisma studio\"",
    "start:prod:prisma": "concurrently --kill-others \"node dist/main\" \"npx prisma studio\""
  },
